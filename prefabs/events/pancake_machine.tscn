[sub_resource type="GDScript" id="GDScript_qnufv"]
resource_name = "event_controller"
script/source = "extends Node

const PASSCODE = \"2FC\"

signal called_to_clear()

@export var animator : AnimationPlayer
@export var container : Control
@export var viewport_container : Control
@export var window : Window

@export var enter_sound : AudioStreamPlayer
@export var passcode_show_button : Button
@export var passcode_container : Control
@export var passcode_parts_contrainers : Array[Label]
@export var cancel_button : Button

func _ready() -> void:
	animator.animation_finished.connect(func (anim):
		if anim == \"drop\":
			GlobalControlls.set_forced_facepick(\"niko_pancakes\")
			passcode_show_button.pressed.connect(func ():
				GlobalControlls.set_forced_facepick(\"niko_huh\")
				enter_sound.play()
				passcode_show_button.text = \"EVENT_PM_SOMETHING_WRONG\"
				passcode_show_button.pressed.connect(func ():
					enter_sound.play()
					passcode_show_button.visible = false
					passcode_container.visible = true
					cancel_button.pressed.connect(func ():
						if passcode_parts_contrainers[0].text + passcode_parts_contrainers[1].text + passcode_parts_contrainers[2].text == PASSCODE:
							GlobalControlls.doned_events += 1
							GlobalControlls.set_forced_facepick(\"niko_pancakes\")
							animator.play(\"true\")
							if not AchievementsGlobalConroller.is_achievement_taked(\"hacked_a_machine\"):
								AchievementsGlobalConroller.take_achievement(\"hacked_a_machine\")
						else:
							animator.play(\"hide\")
					,CONNECT_ONE_SHOT)
					print(\"waiting password\")
				, CONNECT_ONE_SHOT)
			, CONNECT_ONE_SHOT)
		elif not anim == \"RESET\":
			revert()
	)

func _process(_delta: float) -> void:
	if window.visible:
		update_mouse_passthrough_polygon()

func do_event():
	var main_window_screen = DisplayServer.window_get_current_screen(DisplayServer.MAIN_WINDOW_ID)
	var usable_rect = DisplayServer.screen_get_usable_rect(main_window_screen)
	window.size.y = usable_rect.size.y
	window.position.y = usable_rect.position.y
	@warning_ignore(\"integer_division\")
	window.position.x = usable_rect.position.x + usable_rect.size.x / 2 - window.size.x / 2
	#container.position.y = -(1040 - window.size.y)
	window.visible = true
	GlobalControlls.set_forced_facepick(\"niko_huh\")
	animator.play(\"drop\")

func revert():
	GlobalControlls.unforce_facepick()
	called_to_clear.emit()

func update_mouse_passthrough_polygon():
	window.mouse_passthrough_polygon[0].x = 35
	window.mouse_passthrough_polygon[0].y = viewport_container.position.y + container.position.y
	window.mouse_passthrough_polygon[1].x = 365
	window.mouse_passthrough_polygon[1].y = viewport_container.position.y + container.position.y
	window.mouse_passthrough_polygon[2].x = 365
	window.mouse_passthrough_polygon[2].y = viewport_container.position.y + viewport_container.size.y + container.position.y
	window.mouse_passthrough_polygon[3].x = 35
	window.mouse_passthrough_polygon[3].y = viewport_container.position.y + viewport_container.size.y + container.position.y
"

