[sub_resource type="GDScript" id="GDScript_phn8v"]
resource_name = "event_controller"
script/source = "extends Node

@export var screen_hide_window_template : PackedScene
@export var game_animator : AnimationPlayer
@export var screen_texture_rect : TextureRect
var main_screen_shot : ImageTexture

#func _process(_delta: float) -> void:
#	Input.warp_mouse(Vector2i(get_viewport().get_mouse_position()) + Vector2i(randi_range(-2,2),randi_range(-2,2)))

func do_event():
	for i in DisplayServer.get_screen_count():
		if not i == DisplayServer.get_primary_screen():
			var h_window = screen_hide_window_template.instantiate()
			h_window.position = DisplayServer.screen_get_position(i)
			h_window.size = DisplayServer.screen_get_size(i)
			add_child.call_deferred(h_window)
	screen_texture_rect.texture = ImageTexture.create_from_image(DisplayServer.screen_get_image(DisplayServer.get_primary_screen()))
	game_animator.play(\"game\")
"

